<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backdemo.mapper.CountryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.backdemo.entity.Country">
        <id column="id" property="id" />
        <result column="country_iso_code" property="countryIsoCode" />
        <result column="country_short_code" property="countryShortCode" />
        <result column="country_en_name" property="countryEnName" />
        <result column="country_cn_name" property="countryCnName" />
        <result column="currency" property="currency" />
        <result column="area_code" property="areaCode" />
    </resultMap>

</mapper>
